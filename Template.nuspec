<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>jertje260.Core.Project.Template</id>
        <version>0.1.2</version>
        <description>This template generates a solution with multiple projects for .net core including Api</description>
        <authors>Jeroen Broekhuizen</authors>
        <language>en-UK</language>
        <projectUrl>https://github.com/jertje260/dotnet-core-templating</projectUrl>
        <licenseUrl>https://github.com/jertje260/dotnet-core-templating/blob/master/LICENSE.txt</licenseUrl>
        <packageTypes>
            <packageType name="Template" />
        </packageTypes>
    </metadata>
    <files>
        <file src=".\Content\**" target="Content" exclude="**\bin\**;**\obj\**"/>
        <file src=".\LICENSE.txt"/>
        <file src=".\readme.md"/>
    </files>
</package>